package servicios;

import java.util.List;

import javax.jws.WebService;

@WebService(name = "ServidorServicioApuestas", targetNamespace = "http://servicios/")
public interface ServidorServicioApuestas {

	boolean registrarApuesta(Apuesta a);

	int resolverApuestas();//end resolver apuesta

	List<Apuesta> obtenerApuestas();

}