package main;

public class Main {

	public static void main(String[] args) {
		
		//hasta ahora hemos dicho que u es un objeto 
		//de la clase usuario y eso es totalmente cierto.
		Usuario u = new Usuario("ana","ana@gmail.com");
		//pero realmente u es una referencia.
		Usuario u2 = u;
		//la realidad en este ejemplo es que u2, que es una 
		//referencia, apunta al contenido de la referencia u
		//de esta forma decimos de forma coloquila que u2 es 
		//u, o que u2 es lo mismo que u.
		
		u2
		
	}

}
